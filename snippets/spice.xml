<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="spiceos"
           fetch="https://github.com/SpiceOS-Beta"
           revision="12" />

  <remote   name="gitlab"
            fetch="https://gitlab.com/" />

  <remote   name="lineage"
            fetch="https://github.com/LineageOS/"
            revision="lineage-19.0" />

  <remote  name="proton"
           fetch="https://github.com/ProtonAOSP/"
           revision="sc" />

  <!-- External -->
  <project path="external/e2fsprogs" name="android_external_e2fsprogs" groups="pdk" remote="spiceos" />
  <project path="external/mksh" name="android_external_mksh" groups="pdk" remote="spiceos" />
  <project name="android_external_colorkt" path="external/colorkt" remote="proton" groups="proton" />
  <project name="android_external_themelib" path="external/themelib" remote="proton" groups="proton" />

  <!-- Hardware -->
  <project path="hardware/libhardware" name="android_hardware_libhardware" groups="pdk" remote="spiceos" />

  <!-- Packages -->
  <project path="packages/modules/adb" name="android_packages_modules_adb" groups="pdk" remote="lineage" />

  <!-- Vendor -->
  <project path="vendor/qcom/opensource/power" name="android_vendor_qcom_opensource_power" groups="qcom,pdk-qcom" remote="spiceos" />

  <!-- Rootdir -->
  <project path="bionic" name="android_bionic" groups="pdk" remote="spiceos" />
